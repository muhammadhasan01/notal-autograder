PROGRAM max3

KAMUS
    a, b, c: integer
    maks: integer

ALGORITMA
    input(a, b, c)

    if (a >= b) and (b >= c) then
        maks <- a
    else if (a >= c) and (c >= b) then
        maks <- a
    else if (b >= a) and (a >= c) then
        maks <- b
    else if (b >= c) and (c >= a) then
        maks <- b
    else if (c >= a) and (a >= b) then
        maks <- c
    else if (c >= b) and (b >= a) then
        maks <- c

    output(maks)
